{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "baseUrl": "./",
    "moduleResolution": "node",
    "paths": {
      "@/*": ["src/*"],
      "assets/*": ["src/assets/*"],
      "common/*": ["src/common/*"],
      "components/*": ["src/components/*"],
      "network/*": ["src/network/*"],
      "img/*": ["src/assets/img/*"]
    },
    "lib": ["esnext", "dom", "dom.iterable", "scripthost"]
  }
}
